{
    "plugins": [
      "react",
      "jest"
    ],
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "rules": {
        "comma-dangle":["error","always-multiline"],
        "semi":["error","never"],
        "react/prop-types":0,
        "no-unused-vars": ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": true }]
    },
    "env":{
        "browser": true,
        "jest/globals": true,
        "es6": true,
        "node": true
    }
  }