{
    "plugins": [
      "react",
      "jest"
    ],
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "rules": {
        "comma-dangle":["error","always-multiline"],
        "semi":["error","never"]
    },
    "env":{
        "browser": true,
        "jest/globals": true
    }
  }