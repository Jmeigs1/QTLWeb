{
  "name": "qtlweb",
  "version": "1.0.0",
  "description": "QTL Web App",
  "main": "server.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && NODE_ENV=production forever start Server/server.js",
    "stop": "forever stop Server/server.js",
    "client": "webpack-dev-server --mode development --hot --host 0.0.0.0",
    "list": "forever list",
    "git": "npm stop; git fetch && git merge origin/master && npm start",
    "git-reset": "npm stop; git fetch && git reset origin/master --hard && npm start",
    "lint": "eslint ./Client/*.js",
    "lint-fix": "eslint ./Client/*.js --fix",
    "test": "jest --watch",
    "test-update": "jest --updateSnapshot"
  },
